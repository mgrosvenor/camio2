CAKE_GCC41_ZPROFILE_LINKFLAGS="$CAKE_GCC41_PROFILE_LINKFLAGS -lzprofile"
CAKE_GCC41_ZPROFILE_TESTPREFIX=""
CAKE_GCC41_ZPROFILE_POSTPREFIX="timeout 120"


