CAKE_GCC41_DEBUG_LINKFLAGS="$CAKE_GCC41_LINKFLAGS"
CAKE_GCC41_DEBUG_TESTPREFIX="valgrind --quiet --error-exitcode=1"
CAKE_GCC41_DEBUG_POSTPREFIX="timeout 120"

CAKE_GCC41_RELEASE_ID="$CAKE_GCC41_ID"
